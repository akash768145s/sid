{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/components/productList.jsx", "src/app/display/serverc.tsx", "src/app/display/page.Jsx", "src/app/page.jsx", "src/app/Profile/page.jsx", "src/app/upload/page.jsx", "middleware.ts", "src/components/Main/navbar2.tsx"],
  "exclude": ["node_modules"]
}
